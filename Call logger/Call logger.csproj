<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <RootNamespace>Call_logger</RootNamespace>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>static\KUL.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="1200px-KU_Leuven_logo.svg.png" />
    <None Remove="icts.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="static\KUL.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Data.Sqlite.Core" Version="8.0.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="8.0.0" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="static\1200px-KU_Leuven_logo.svg.png" />
    <Resource Include="static\icts.png" />
  </ItemGroup>

</Project>
